    "node-ts-cache": "^4.4.0",
    "node-ts-cache-storage-memory": "^4.4.0",
    "node-ts-cache-storage-node-fs": "^4.4.0",
    "prettier": "^3.4.2",
    "type-fest": "^4.32.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/expect": "^24.3.0",
    "@types/mocha": "10.0.6",
    "@types/node": "20.8.8",
    "mocha": "10.2.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
